Algoritmo SUMA_NUMEROS
	Definir A Como Entero;
	Definir B Como Entero;
	Definir Suma Como Entero;
	//Ingreso de Datos
	Escribir"Ingrese Primer numero de Operacion Suma";
	Leer A;
	Escribir"Ingrese Segundo numero de Operacion Suma";
